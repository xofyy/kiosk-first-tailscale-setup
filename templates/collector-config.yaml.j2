# ACO Maintenance Panel - collector-agent Configuration
# Generated automatically - do not edit manually
# /etc/collector-agent/config.yaml

# Collection interval (seconds)
interval: {{ interval }}

# Node exporter
node_exporter:
  enabled: true
  port: 9100

# NVIDIA SMI metrics
nvidia_smi:
  enabled: true

# System metrics
system:
  cpu: true
  memory: true
  disk: true
  network: true

# Output
output:
  type: prometheus
  port: 9101
