<?xml version="1.0" encoding="UTF-8"?>
<!-- Kiosk Setup Panel - Openbox RC Configuration -->
<!-- Generated automatically - do not edit manually -->

<openbox_config xmlns="http://openbox.org/3.4/rc">
  <keyboard>
    <!-- Switch to Panel: Ctrl+Alt+P -->
    <keybind key="C-A-p">
      <action name="Execute">
        <command>/usr/local/bin/switch-to-panel.sh</command>
      </action>
    </keybind>

    <!-- Switch to Kiosk: Ctrl+Alt+K -->
    <keybind key="C-A-k">
      <action name="Execute">
        <command>/usr/local/bin/switch-to-kiosk.sh</command>
      </action>
    </keybind>

    <!-- Switch to Cockpit: Ctrl+Alt+C -->
    <keybind key="C-A-c">
      <action name="Execute">
        <command>/usr/local/bin/switch-to-cockpit.sh</command>
      </action>
    </keybind>
  </keyboard>

  <applications>
    <!-- All windows undecorated and fullscreen -->
    <application class="*">
      <decor>no</decor>
      <fullscreen>yes</fullscreen>
    </application>
  </applications>
</openbox_config>
